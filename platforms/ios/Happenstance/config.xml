<?xml version='1.0' encoding='utf-8'?>
<widget id="hu.dpal.starter" version="0.0.2" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="LocalStorage">
        <param name="ios-package" value="CDVLocalStorage" />
    </feature>
    <feature name="HandleOpenUrl">
        <param name="ios-package" value="CDVHandleOpenURL" />
        <param name="onload" value="true" />
    </feature>
    <feature name="IntentAndNavigationFilter">
        <param name="ios-package" value="CDVIntentAndNavigationFilter" />
        <param name="onload" value="true" />
    </feature>
    <feature name="GestureHandler">
        <param name="ios-package" value="CDVGestureHandler" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Keyboard">
        <param name="ios-package" onload="true" value="IonicKeyboard" />
    </feature>
    <feature name="SplashScreen">
        <param name="ios-package" value="CDVSplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <feature name="InAppBrowser">
        <param name="ios-package" value="CDVInAppBrowser" />
    </feature>
    <name>Facebook-demo</name>
    <description>
        Sample PhoneGap / Cordova application built with Angular and Ionic Framework, uses RequireJS for loading scripts. Example controllers, filters, services, directives and config.
    </description>
    <author email="" href="http://dpal.hu">
        Domonkos Pal
    </author>
    <content src="index.html" />
    <icon src="www/resources/icon/app-icon-80.png" />
    <icon gap:platform="android" gap:qualifier="ldpi" src="www/resources/android/icon/icon-36.png" />
    <icon gap:platform="android" gap:qualifier="mdpi" src="www/resources/android/icon/icon-48.png" />
    <icon gap:platform="android" gap:qualifier="hdpi" src="www/resources/android/icon/icon-72.png" />
    <icon gap:platform="android" gap:qualifier="xhdpi" src="www/resources/android/icon/icon-96.png" />
    <icon gap:platform="ios" height="57" src="www/resources/ios/icon/icon-57.png" width="57" />
    <icon gap:platform="ios" height="72" src="www/resources/ios/icon/icon-72.png" width="72" />
    <icon gap:platform="ios" height="114" src="www/resources/ios/icon/icon-114.png" width="114" />
    <icon gap:platform="ios" height="144" src="www/resources/ios/icon/icon-114.png" width="144" />
    <icon gap:platform="webos" src="www/resources/ios/icon/icon-72.png" />
    <gap:splash gap:platform="android" gap:qualifier="port-ldpi" src="www/resources/android/splash/splace-screen.png" />
    <gap:splash gap:platform="android" gap:qualifier="port-mdpi" src="www/resources/android/splash/splace-screen.png" />
    <gap:splash gap:platform="android" gap:qualifier="port-hdpi" src="www/resources/android/splash/splace-screen.png" />
    <gap:splash gap:platform="android" gap:qualifier="port-xhdpi" src="www/resources/android/splash/splace-screen.png" />
    <gap:splash gap:platform="ios" height="480" src="www/resources/splash/ios/splace-screen.png" width="320" />
    <gap:splash gap:platform="ios" height="960" src="www/resources/splash/ios/splace-screen.png" width="640" />
    <gap:splash gap:platform="ios" height="1136" src="www/resources/splash/ios/splace-screen.png" width="640" />
    <gap:splash gap:platform="ios" height="1024" src="www/resources/splash/ios/splace-screen.png" width="768" />
    <gap:splash gap:platform="ios" height="768" src="www/resources/splash/ios/splace-screen.png" width="1024" />
    <access origin="*" />
    <preference name="AllowInlineMediaPlayback" value="false" />
    <preference name="BackupWebStorage" value="cloud" />
    <preference name="DisallowOverscroll" value="false" />
    <preference name="EnableViewportScale" value="false" />
    <preference name="KeyboardDisplayRequiresUserAction" value="true" />
    <preference name="MediaPlaybackRequiresUserAction" value="false" />
    <preference name="SuppressesIncrementalRendering" value="false" />
    <preference name="SuppressesLongPressGesture" value="false" />
    <preference name="Suppresses3DTouchGesture" value="false" />
    <preference name="GapBetweenPages" value="0" />
    <preference name="PageLength" value="0" />
    <preference name="PaginationBreakingMode" value="page" />
    <preference name="PaginationMode" value="unpaginated" />
    <preference name="permissions" value="none" />
    <preference name="phonegap-version" value="3.5.0" />
    <preference name="orientation" value="default" />
    <preference name="target-device" value="universal" />
    <preference name="fullscreen" value="true" />
    <preference name="webviewbounce" value="false" />
    <preference name="prerendered-icon" value="true" />
    <preference name="stay-in-webview" value="false" />
    <preference name="ios-statusbarstyle" value="black-opaque" />
    <preference name="detect-data-types" value="true" />
    <preference name="exit-on-suspend" value="false" />
    <preference name="show-splash-screen-spinner" value="true" />
    <preference name="auto-hide-splash-screen" value="true" />
    <preference name="disable-cursor" value="false" />
    <preference name="android-minSdkVersion" value="7" />
    <preference name="android-installLocation" value="auto" />
    <preference name="ShowSplashScreen" value="true" />
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="3000" />
    <preference name="AutoHideSplashScreen" value="true" />
    <preference name="SplashShowOnlyFirstTime" value="false" />
    <preference name="FadeSplashScreen" value="false" />
</widget>
